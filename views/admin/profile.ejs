 	 <div class="profile">

		<div class="profile-bottom">
			<h3><i class="fa fa-user"></i><%=title%></h3>
			<div class="profile-bottom-top">
			<div class="col-md-4 profile-bottom-img">
				<img src="../uploads/profile/<%=userPhoto%>" width="150" height="150" alt="">
			</div>
			<div class="col-md-8 profile-text">
				<h6><%=userFirstName%> <%=userLastName%></h6>
				<table>
				<tr><td>Role</td>  
				<td>:</td>  
				<td><%=role%></td></tr>
				
				<tr>
				<td>Email</td>
				<td> :</td>
				<td><a href="mailto:<%=userEmail%>"><%=userEmail%></a></td>
				</tr>
				<tr>
				<td>Telephone</td>
				<td> :</td>
				<td> <%=userPhone%></td>
				</tr>
				<tr>
				<td>Username </td>
				<td>:</td>
				<td> <%=userLogin%></td>
				</tr>
				</table>
			</div>
			<div class="clearfix"></div>
			</div>
